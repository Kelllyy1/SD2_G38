/*Results*/

QScrollArea {
        border: 2px solid #cccccc;  /* Optional border */
    }
    QScrollBar:vertical {
        border: none;
        background: #f0f0f0;
        width: 8px;
        margin: 2px 0 2px 0;
    }
    QScrollBar::handle:vertical {
        background: #888;
        min-height: 20px;
    }
    QScrollBar::handle:vertical:hover {
        background: #555;
    }
    QScrollBar::sub-line:vertical, QScrollBar::add-line:vertical {
        background: none;
        height: 0px;
    }

    QWidget#scrollDetails{
        background-color: transparent;
        border: none;
    }
    QScrollArea{
        background-color: transparent;
