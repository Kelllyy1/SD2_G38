/* Home */

/* Apply styles to the main window */
QWidget#window {
    background-color: #fcf4eb;
}

QWidget#left {
    background-color: transparent;
    max-width: 200px;;
    min-width: 200px;
}

QWidget#right {
    background-color: transparent;
}

QWidget#topRight {
    background-color: #fefaf5;
    border: 1px solid #decdbc;
    border-radius: 10px;
    min-width: 100%;
}
QWidget#bottomRight {
    background-color: #fefaf5;
    border: 1px solid #decdbc;
    border-radius: 10px;
}
QWidget#interact {
   background-color: #fefaf5;
    border: 1px solid #decdbc;
    border-radius: 10px;
}
QWidget#interactInput {
    background-color: white;
        border: 1px solid #decdbc;
    border-radius: 3px;
}
QWidget#interactButton {
    background: qlineargradient(spread:pad,
      x1:0, y1:0, x2:1, y2:0,
      stop:0 #ffad6b,
      stop:1 #ff7a98);
      color: white;
      border: 1px solid #ff6252;
      padding: 3px;
    min-width: 60px;
    max-width: 60px;
    border-radius: 5px;
}
/* Hover effect */
QWidget#interactButton:hover {
    border: 2px solid #00FFFF; /* A slightly different border color on hover */
}

/* Click effect */
QWidget#interactButton:pressed {
    transform: scale(1.05); /* Increases the size by 1% on click */
    border: 2px solid #388e3c; /* Darker border color on click */
}
